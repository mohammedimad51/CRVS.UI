@model IEnumerable<FacilityType>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>



<div>

    <a asp-action="Create" asp-asp-controller="Governorates" class="btn btn-primary">Create new Governorate</a>

</div>


<table class="table table-bordered table-hover">



    <tr>

     
        <th>@Html.DisplayNameFor(c=>c.FacilityTypeName)</th>



    </tr>
    @foreach (var item in Model)
    {
        <tr>

            <td>  @item.FacilityTypeName</td>


        </tr>
        <tr>
         <td>
                <a asp-route-id="@item.FacilityTypeId" class="btn btn-warning btn-sm" asp-action="Edit">Edit</a>
                <a asp-route-id="@item.FacilityTypeId" class="btn btn-danger btn-sm" asp-action="Delete">Delete</a>
                <a asp-route-id="@item.FacilityTypeId" class="btn btn-secondary btn-sm" asp-action="Details">Details</a>

                    </td>
                </tr>
    }

</table>





